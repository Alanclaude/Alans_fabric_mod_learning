{
	"schemaVersion": 1,
	"id": "alanmod",
	"version": "${version}",
	"name": "Alan's frist mod",
	"description": "This is an example description! Tell everyone what your mod is about! A: For learn!",
	"authors": [
		"Alan_Claude"
	],
	"contact": {
		"homepage": "https://github.com/Alanclaude/Alans_fabric_mod_learning",
		"sources": "https://github.com/Alanclaude/Alans_fabric_mod_learning"
	},
	"license": "CC0-1.0",
	"icon": "assets/modid/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
          "fan.alanmod.ExampleMod"
		],
		"client": [
			"fan.alanmod.ExampleModClient"
		]
	},
	"mixins": [
		"modid.mixins.json",
		{
			"config": "modid.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.14.21",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}